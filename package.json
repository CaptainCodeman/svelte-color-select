{
  "name": "svelte-color-select",
  "description": "Okhsl color select for Svelte",
  "version": "0.0.10",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "svelte",
    "oklab",
    "kkhsl",
    "hsl",
    "color",
    "color-picker",
    "color-select"
  ],
  "homepage": "https://captaincodeman.github.io/svelte-color-select/",
  "repository": {
    "type": "git",
    "url": "https://github.com/captaincodeman/svelte-color-select.git"
  },
  "bugs": {
    "url": "https://github.com/captaincodeman/svelte-color-select/issues",
    "email": "simon@captaincodeman.com"
  },
  "author": {
    "name": "Simon Green",
    "email": "simon@captaincodeman.com",
    "url": "https://www.captaincodeman.com/"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.0",
    "@sveltejs/package": "^2.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "publint": "^0.1.12",
    "svelte": "^4.0.0-next.0",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "hsluv": "^1.0.0"
  }
}